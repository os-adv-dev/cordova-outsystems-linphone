    <!-- ios -->
    <platform name="ios">
        <config-file target="config.xml" parent="/*">
            <feature name="Linphone">
                <param name="ios-package" value="Linphone"/>
            </feature>
        </config-file>

        <podspec>
            <config>
                <source url="https://gitlab.linphone.org/BC/public/podspec.git" />
            </config>
            <pods use-frameworks="true">
                <pod name="linphone-sdk" spec="~> 5.0" />
            </pods>
        </podspec>

        <config-file target="*-Info.plist" parent="NSCameraUsageDescription">
			<string>This app needs access to the camera for video calls.</string>
		</config-file>
		<config-file target="*-Info.plist" parent="NSMicrophoneUsageDescription">
			<string>This app needs access to the microphone for audio calls.</string>
		</config-file>
        
        <!--preference name="USESWIFTLANGUAGEVERSION" value="5" default="5"/-->
        <dependency id="cordova-plugin-add-swift-support" version="2.0.2"/>
        <hook type="after_plugin_install" src="hooks/outsystems/ios/handle_swift_import.js" />

        <config-file target="*-Info.plist" parent="UIBackgroundModes">
            <array>
                <string>remote-notification</string>
            </array>
        </config-file>
        <config-file target="*-Debug.plist" parent="aps-environment">
            <string>development</string>
        </config-file>
        <config-file target="*-Release.plist" parent="aps-environment">
            <string>production</string>
        </config-file>
        
        
